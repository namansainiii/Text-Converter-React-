<Textarea mode={mode} showalert={showalert}/>   \n          </Route>\n      </Switch>\n        \n      \n    </div>\n\n</Router>\n    </>\n  );\n}\n\nexport default App;\n","D:\\CSE\\reactjs\\my-react-project\\src\\components\\Alert.js",[],[],"D:\\CSE\\reactjs\\my-react-project\\src\\components\\About.js",[],[],"D:\\CSE\\reactjs\\my-react-project\\src\\components\\Textarea.js",[],[],"D:\\CSE\\reactjs\\my-react-project\\src\\components\\Navbar.js",["40"],[],"import React, { useState } from 'react'\r\nimport { Link } from 'react-router-dom';\r\nexport default function Navbar(props) {\r\n  return (\r\n<nav className={`navbar navbar-expand-lg navbar-${props.mode} bg-${props.mode}`}>\r\n<div className=\"container-fluid\">\r\n    <Link className=\"navbar-brand\" to=\"/\">{props.title}</Link>\r\n\r\n    <ul className=\"navbar-nav mr-auto\">\r\n      <li className=\"nav-item \">\r\n      <Link className=\"nav-link active\" to=\"/\">Home</Link>\r\n      \r\n        </li>\r\n      <li className=\"nav-item\">\r\n        <Link  className=\"nav-link\" to =\"/about\">About</Link>\r\n      </li>\r\n    </ul>\r\n\r\n<div className={`form-check form-switch text-${props.mode==='light'?'dark':'light'}`}>\r\n     <input className=\"form-check-input\" onClick={props.toggle} type=\"checkbox\" id=\"flexSwitchCheckDefault\"/>\r\n     <label className=\"form-check-label\"  htmlFor=\"flexSwitchCheckDefault\">Dark/Light Mode</label> \r\n</div>\r\n\r\n</div>\r\n</nav>\r\n  )\r\n}\r\n",{"ruleId":"41","severity":1,"message":"42","line":12,"column":3,"nodeType":"43","messageId":"44","endLine":12,"endColumn":7},{"ruleId":"41","severity":1,"message":"45","line":1,"column":17,"nodeType":"43","messageId":"44","endLine":1,"endColumn":25},"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used."]